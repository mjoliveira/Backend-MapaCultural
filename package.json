{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "jest --coverage",
    "test-no-color": "jest --no-color --coverage",
    "lint": "eslint src/**",
    "lint-fix": "eslint src/** --fix",
    "migration": "sequelize db:migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "8.1.0",
    "express": "4.18.2",
    "morgan": "^1.9.1",
    "mysql2": "1.6.5",
    "sequelize": "5.15.1",
    "swagger-jsdoc": "3.4.0",
    "swagger-ui-express": "4.0.7"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "eslint": "^6.2.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.4",
    "sequelize-cli": "^5.5.0",
    "sequelize-mock": "^0.10.2"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "coverageReporters": [
      "text",
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "src/business/**/*.js"
    ]
  }
}
